<h1>Hello World!</h1>
{% if latest_list %}
    <ul>
    {% for item in latest_list %}
    <TABLE bgColor=red height=20 width={{item.temp}} cellSpacing=0 cellPadding=0 border= 0> <TR><TD></TD></TR>
    	<p> id = {{item.id}} date :  {{item.date}} valeur : {{item.temp}}
        
        </p>
    {% endfor %}
    </ul>
{% else %}
    <p>No item are available.</p>
{% endif %}



